#
# Interface between Oil and Water
#

tolerance 2.0 
filetype pdb
output hexaneWater.pdb

structure ../pdbs/water.pdb 
  number 900
  inside box 0. 0. 0. 30. 30. 30.
end structure

structure ../pdbs/hexane.pdb
  number 124
  inside box 0. 0. 30. 30. 30. 60.
end structure

structure ../pdbs/water.pdb
  number 900
  inside box 0. 0. 60. 30. 30. 90.
end structure
